{
  "name": "sagsoz06/news-module",
  "type": "asgard-module",
  "description": "A news module for CMS.",
  "keywords": [
    "news",
    "journal"
  ],
  "license": "MIT",
  "authors": [],
  "require": {
    "php": ">=5.6",
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "orchestra/testbench": "3.3.*",
    "fzaninotto/faker": "~1.5",
    "phpro/grumphp": "^0.9.1",
    "friendsofphp/php-cs-fixer": "^1.11"
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\News\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-2.0": "2.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
